title: Preserve Original File Timestamp
author: kellnerd
script_language_version: '1.1'
script: "$noop( Write original file timestamp as tag if it does not already exist\
  \ )\n$if($not(%originalfiletime%),\n\t$set(originalfiletime,$if($lt(%_file_modified_timestamp%,%_file_created_timestamp%),%_file_modified_timestamp%,%_file_created_timestamp%))\n\
  )\n"
